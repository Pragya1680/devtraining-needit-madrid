        <description>To validate the email address.</description>
        <name>emailValidation</name>
        <script><![CDATA[function emailValidation(email){
	
	if(email.match(/^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/)){
			return true;
	}
	
	else 
		return false;
}]]></script>
